    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "types": ["node"],
    "esModuleInterop": true
  },
  "include": ["vite.config.ts", "src/**/*"]
}