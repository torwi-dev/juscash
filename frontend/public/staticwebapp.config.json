{
  "staticFiles": {
    "headers": {
      "*.js": {
        "Content-Type": "application/javascript; charset=utf-8"
      },
      "*.mjs": {
        "Content-Type": "application/javascript; charset=utf-8"
      },
      "*.css": {
        "Content-Type": "text/css; charset=utf-8"
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
